{
  "name": "react-matrix-code",
  "version": "1.0.0",
  "description": "High-performance, simulated code rain from The Matrix.",
  "keywords": [
    "matrix",
    "code",
    "rain"
  ],
  "author": "Daniel Spajic",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dspacejs/react-matrix-code.git"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "develop": "yarn build --watch",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "lint": "eslint -c .eslintrc.js .",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier -c \"**/*.js\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:fix": "yarn prettier -- --write",
    "validate": "yarn lint && yarn prettier && yarn test",
    "deploy": "yarn validate && yarn build",
    "install-hooks": "husky install"
  },
  "files": [
    "/dist"
  ],
  "directories": {
    "dist": "./dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "15.14.0",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
