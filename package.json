    "remark-preset-lint-recommended": "^6.0.0",
    "rollup": "^2.56.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
